<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ShoppingCart.DAL" assembly="ShoppingCart.DAL">
  <class name="Customer" table ="Customer">
    <id name="Id">
      <generator class="identity"/>
    </id>
    <property name="Name" column="Name"/>
    <property name="Email" unique="true" column="Email"/>
    <property name="Card" column="Card"/>
<!--    <property name="Card" type="ShoppingCart.DAL.UserTypes.EncryptedLongAsString" column="Card"/>-->
  </class>
</hibernate-mapping>